from sklearn.ensemble import RandomForestClassifier



features = [
    [150, 0],  # Apple
    [170, 0],  # Apple
    [130, 1],  # Orange
    [120, 1],  # Orange
]


labels = ["apple", "apple", "orange", "orange"]


clf = RandomForestClassifier()


clf = clf.fit(features, labels)


prediction = clf.predict([[130,0]])


print(f"The fruit is {prediction[0]}")


